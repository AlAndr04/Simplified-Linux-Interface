package input;
import dirCommands.Commands;
public class ExecuteHistory extends Commands<Void>{
  private int numcommand;
  public ExecuteHistory(String numhistory) {
    try {
      // Makes start the size minus newstart.
      this.numcommand = Integer.parseInt(numhistory) - 1;
      Input.HistoryArr.remove(Input.HistoryArr.size() - 1);
      // If newstart is not a number, throws an exception to output and
      // makes newstart bigger than history array.
    } catch (NumberFormatException e) {
      Output.mustBeIntegers();
    }
  }
  public Void execute() {
    if ((numcommand >= 0) && (numcommand < Input.HistoryArr.size())) {
      Input.HistoryArr.add(Input.HistoryArr.get(numcommand));
      CommandManager command = new CommandManager(Input.HistoryArr.get(numcommand));
    }
    return null;
  }
}
