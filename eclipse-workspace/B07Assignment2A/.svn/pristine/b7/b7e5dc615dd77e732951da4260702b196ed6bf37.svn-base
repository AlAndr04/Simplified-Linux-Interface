package test.input;
import static org.junit.Assert.*;
import java.net.MalformedURLException;
import java.net.URISyntaxException;

import org.junit.Before;
import org.junit.Test;
import dirCommands.Cd;
import fileSystemObjects.*;



public class CdTest {
    private Cd cd;
    private MockJShell JShell;
    @Before
    public void setUp() {
        JShell = new MockJShell();
    }
    
    @Test
    public void testExecuteNoDirectory() {
        cd = new Cd("/Jeff");
        Directory output = cd.executeHelper(JShell.rootDir, JShell.rootDir);
        assertEquals(output.getName(), "/");
    }
    @Test
    public void testExecuteDirectoryExists() {
      cd = new Cd("/Dir_1");
      Directory output = cd.executeHelper(JShell.rootDir, JShell.rootDir);
      String actualOutput = "Dir_1";
      assertEquals(actualOutput, output.getName());
  }
    @Test
    public void testExecuteDot() {
      cd = new Cd(".");
      cd.execute();
      String actualOutput = JShell.;
      assertEquals(actualOutput, output.getName());
  }
    @Test
    public void testExecuteDoubleDotValid() {
      cd = new Cd("/Dir_1", JShell.MockLocation);
      Directory output = cd.executeHelper(JShell.rootDir, JShell.rootDir);      
      cd = new Cd("..");
      output = cd.executeHelper(JShell.rootDir, output);
      String actualOutput = "/";
      assertEquals(actualOutput, output.getName());
  }
    @Test
    public void testExecuteDoubleDotInvalid() {
      cd = new Cd("..");
      Directory output = cd.executeHelper(JShell.rootDir, JShell.rootDir);
      String actualOutput = "/";
      assertEquals(actualOutput, output.getName());
  }
    @Test
    public void testExecuteMultipleDirectories() {
      cd = new Cd("/Dir_1/Dir_1_1");
      Directory output = cd.executeHelper(JShell.rootDir, JShell.rootDir);
      String actualOutput = "Dir_1_1";
      assertEquals(actualOutput, output.getName());
  }
    @Test
    public void testExecuteCdIntoFile() {
      cd = new Cd("/Dir_1/file_1_1");
      Directory output = cd.executeHelper(JShell.rootDir, JShell.rootDir);
      String actualOutput = "/";
      assertEquals(JShell.rootDir, output);
  }
    @Test
    public void testExecuteMultipleCds() {
      cd = new Cd("/Dir_1");
      Directory output = cd.executeHelper(JShell.rootDir, JShell.rootDir);
      cd = new Cd("/Dir_1/Dir_1_1");
      output = cd.executeHelper(JShell.rootDir, output);
      assertEquals("Dir_1_1", output.getName());
  }   
}
